{
  "env": {
    "es6": true
  },
  "extends": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": true,
        "trailingComma": "es5",
        "tabWidth": 2
      }
    ]
  }
}