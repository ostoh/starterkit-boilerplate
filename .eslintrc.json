{
    "globals": {
        "jQuery": "readonly",
        "$": "readonly"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "commonjs": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", { "endOfLine": "auto" }],
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}